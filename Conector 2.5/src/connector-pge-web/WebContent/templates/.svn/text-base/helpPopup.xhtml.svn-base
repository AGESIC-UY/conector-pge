<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:rich="http://richfaces.org/rich">
	
	<rich:modalPanel width="750" height="350" id="panel" style="overflow-y:auto;overflow-x:hidden">
		<f:facet name="header">
			<h:panelGroup>
				<h:outputText value="Ayuda"></h:outputText>
			</h:panelGroup>
		</f:facet>
		<f:facet name="controls">
			<h:panelGroup>
				<h:graphicImage value="../img/cross.png" styleClass="hidelink"
					id="hidelink" />
				<rich:componentControl for="panel" attachTo="hidelink"
					operation="hide" event="onclick" />
			</h:panelGroup>
		</f:facet>
		<ui:include src="#{help.helpViewPath}"/>
		
	</rich:modalPanel>
	
	
	
</ui:composition>
